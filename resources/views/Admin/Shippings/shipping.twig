{% extends '@CartRabbit/admin_template.html.twig' %}

 {% block content %}

 {% endblock %}

{% block body %}
    <div class="flylshop">
        <div class="container">
            <div class="row">
                <div class="col-md-11 flylshop">
                    <div class="panel panel-info">
                        <div class="panel-body">
                            {% include '@CartRabbit/Admin/Includes/Configuration/configMenu.twig' %}
                            <div class="">
                                <div class="row">
                                    <div class="col-md-12 flylshop">
                                        <form method="post" action="{{ site_addr }}/config/shipping">
                                            <div class="panel panel-info">
                                                <div align="right">
                                                    <button class="btn btn-primary">Save</button>
                                                </div>
                                                {% include '@CartRabbit/Admin/Includes/Configuration/Shipping/optionMenu.twig' %}
                                                <div class="panel-body">
                                                    {% autoescape %}
                                                        {{ content | raw }}
                                                    {% endautoescape %}
                                                </div>
                                            </div>
                                            <input type="hidden" value="{{ plugin_type }}"
                                                   name="cartrabbit[shipping][plugin]">
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    </div>


{% endblock %}